{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "moduleResolution": "node",

        "declaration": false,
        "lib": [
            "es5",
            "es2015.promise",
            "dom"
        ],

        "strict": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,

        "skipLibCheck": true,
        "importHelpers": true,
        "jsx": "react",
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true
    }
}