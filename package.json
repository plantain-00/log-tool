{
  "name": "log-tool",
  "version": "2.0.0",
  "description": "A tool to watch log files changes, and transport, transform, store, monitor, search logs.",
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf dist && npm run file2variable && tsc -p src && tsc -p static && webpack --display-modules --config static/webpack.config.js && npm run bundle-vendor-css && npm run bundle-css && npm run clean && rev-static --config static/rev-static.config.js",
    "lint": "tslint \"src/*.ts\" \"static/*.ts\"",
    "file2variable": "file2variable-cli static/app.template.html static/protocol.proto static/protocol.json -o static/variables.ts --html-minify --json --protobuf",
    "clean": "rimraf static/vendor.bundle-*.js static/index.bundle-*.js static/vendor.bundle-*.css static/index.bundle-*.css",
    "bundle-vendor-css": "cleancss ./node_modules/bootstrap/dist/css/bootstrap.min.css ./node_modules/github-fork-ribbon-css/gh-fork-ribbon.css -o ./static/vendor.bundle.css",
    "bundle-css": "cleancss ./static/index.css -o ./static/index.bundle.css",
    "doc": "dot -Tpng architecture.dot > architecture.png"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/log-tool.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/log-tool/issues"
  },
  "homepage": "https://github.com/plantain-00/log-tool#readme",
  "devDependencies": {
    "@types/body-parser": "1.16.3",
    "@types/chart.js": "2.4.2",
    "@types/express": "4.0.35",
    "@types/lodash": "4.14.63",
    "@types/node": "7.0.14",
    "@types/node-fetch": "1.6.7",
    "@types/sqlite3": "2.2.33",
    "@types/uws": "0.13.0",
    "@types/ws": "0.0.40",
    "bootstrap": "3.3.7",
    "chart.js": "2.5.0",
    "clean-css-cli": "4.0.12",
    "file2variable-cli": "1.3.0",
    "github-fork-ribbon-css": "0.2.1",
    "rev-static": "2.1.2",
    "rimraf": "2.6.1",
    "rpc-on-ws": "3.1.1",
    "tslint": "5.1.0",
    "typescript": "2.3.2",
    "vue": "2.3.0",
    "vue-class-component": "5.0.1",
    "webpack": "2.4.1"
  },
  "dependencies": {
    "ajv": "5.0.1",
    "body-parser": "1.17.1",
    "express": "4.15.2",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "node-fetch": "1.6.3",
    "protobufjs": "6.7.3",
    "reconnection": "1.0.0",
    "rxjs": "5.3.0",
    "sqlite3": "3.1.8",
    "tslib": "1.6.1",
    "uws": "0.14.5",
    "ws": "2.3.1"
  }
}
